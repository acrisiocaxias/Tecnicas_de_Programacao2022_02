//1° - public class q1 {
public static void main(String[] args) {
// TODO Auto-generated method stub
int A = 20;
int B = 0;
if(A > 10) {
System.out.println("A > 10");
}
if((A+B)==20) {
System.out.println("A + B == 20");
}
if((A<10)&&(A+B)!=20) {
System.out.println("Numeros não validos");
}
}
}

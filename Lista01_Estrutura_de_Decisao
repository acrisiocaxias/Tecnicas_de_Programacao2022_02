}
}
}
4°- public class q4 {
public static void main(String[] args) {
// TODO Auto-generated method stub
int A = 9;
int B = 11;
if(A > 10) {
System.out.println("A > 10");
}else {
System.out.println("A <= 10");
}
if((A+B)== 20) {
System.out.println("A + B == 20");
}else {
System.out.println(" A + B != 20");
}
}
